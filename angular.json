{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "ngx-nations-showcase",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "ngx-nations-showcase": {
      "projectType": "application",
      "root": "apps/ngx-nations-showcase",
      "sourceRoot": "apps/ngx-nations-showcase/src",
      "prefix": "ngx-nations",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/ngx-nations-showcase",
            "index": "apps/ngx-nations-showcase/src/index.html",
            "main": "apps/ngx-nations-showcase/src/main.ts",
            "polyfills": "apps/ngx-nations-showcase/src/polyfills.ts",
            "tsConfig": "apps/ngx-nations-showcase/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngx-nations-showcase/src/favicon.ico",
              "apps/ngx-nations-showcase/src/assets"
            ],
            "styles": ["apps/ngx-nations-showcase/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngx-nations-showcase/src/environments/environment.ts",
                  "with": "apps/ngx-nations-showcase/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "ngx-nations-showcase:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-nations-showcase:build:production"
            }
          }
        },
        "analyze": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "nx run ngx-nations-showcase:build --prod --named-chunks --source-map",
                "forwardAllArgs": false
              },
              {
                "command": "source-map-explorer './dist/apps/ngx-nations-showcase/*.js' --html tmp/result.html"
              }
            ]
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-nations-showcase:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ngx-nations-showcase/src/**/*.ts",
              "apps/ngx-nations-showcase/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ngx-nations-showcase"],
          "options": {
            "jestConfig": "apps/ngx-nations-showcase/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngx-nations-showcase-e2e": {
      "root": "apps/ngx-nations-showcase-e2e",
      "sourceRoot": "apps/ngx-nations-showcase-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngx-nations-showcase-e2e/cypress.json",
            "tsConfig": "apps/ngx-nations-showcase-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngx-nations-showcase:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-nations-showcase:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ngx-nations-showcase-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "icon": {
      "projectType": "library",
      "root": "libs/icon",
      "sourceRoot": "libs/icon/src",
      "prefix": "ngx-nations",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/icon/tsconfig.lib.json",
            "project": "libs/icon/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/icon/tsconfig.lib.prod.json"
            }
          }
        },
        "build-with-deps": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "nx run core:build-with-deps"
              },
              {
                "command": "nx run icon:build:production"
              },
              {
                "command": "yarn run svg-to-ts"
              }
            ]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/icon/src/**/*.ts",
              "libs/icon/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/icon"],
          "options": {
            "jestConfig": "libs/icon/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "projectType": "library",
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "prefix": "ngx-nations",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/core/tsconfig.lib.json",
            "project": "libs/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/tsconfig.lib.prod.json"
            }
          }
        },
        "build-with-deps": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "cd node_modules/i18n-iso-countries && yarn build"
              },
              {
                "command": "nx run core:build:production"
              }
            ]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/**/*.ts",
              "libs/core/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nation-flag": {
      "root": "libs/nation-flag",
      "sourceRoot": "libs/nation-flag/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nation-flag/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nation-flag"],
          "options": {
            "jestConfig": "libs/nation-flag/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nation-flag-angular": {
      "projectType": "library",
      "root": "libs/nation-flag-angular",
      "sourceRoot": "libs/nation-flag-angular/src",
      "prefix": "ngx-nations",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/nation-flag-angular/tsconfig.lib.json",
            "project": "libs/nation-flag-angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/nation-flag-angular/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nation-flag-angular/src/**/*.ts",
              "libs/nation-flag-angular/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nation-flag-angular"],
          "options": {
            "jestConfig": "libs/nation-flag-angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
